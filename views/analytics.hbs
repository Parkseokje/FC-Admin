<div class="row home_section">
  <div class="col-xs-12">
    <div class="box">
      <div class="box-body table-responsive">
        <table id="table_analytics" class="table no-margin table-bordered table-striped" width="100%">
          <thead>
            <tr>
              <th class="center">거래처명</th>
              <th class="center">년월</th>
              <th class="center">이용자수</th>
              <th class="center">메세지전송</th>
            </tr>
          </thead>
          <tbody>
            {{#each list}}
            <tr>
              <td>{{fc_name}}</td>
              <td>{{yearmonth}}</td>
              <td>{{user_cnt}}</td>
              <td>{{message_cnt}}</td>
            </tr>
            {{/each}}
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>