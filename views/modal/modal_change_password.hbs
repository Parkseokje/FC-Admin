<!--agent password chagne-->
<div class="modal fade" id="setEmployeePassword" role="dialog">
    <form class="modal-dialog" method="post" action="" id="frm_set_employee_password">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span></button>
                <h4 class="modal-title">비밀번호 변경</h4>
            </div>
            <div class="modal-body">
                <div class="form-group">
                    <label>비밀번호</label>
                    <input type="password" class="form-control pass" name="pass" id="set_password" placeholder="4글자 이상 입력" required autocomplete="off" />
                    <!--<small class="text-red">* 최소 4자리 이상 입력해주세요.</small>-->
                </div>
                <div class="form-group">
                    <label>비밀번호 확인</label>
                    <input type="password" class="form-control re_pass" name="re_pass" placeholder="비밀번호 재입력" required autocomplete="off" />
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default pull-left" data-dismiss="modal">닫기</button>
                <button type="submit" class="btn btn-primary btn-set-agent-password-submit">변경</button>
            </div>
        </div>
        <!-- /.modal-content -->
        <input type="hidden" name="user_id" class="user_id" />
        <input type="hidden" name="user_name" class="user_name" />
    </form>
    <!-- /.modal-dialog -->
</div>
<!-- /.modal -->